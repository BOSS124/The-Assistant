<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pm.assistant"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="27" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.pm.assistant.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.pm.assistant.Register1Activity" />
        <activity android:name="com.example.pm.assistant.Register2Activity" />
        <activity android:name="com.example.pm.assistant.Register3Activity" />
        <activity
            android:name="com.example.pm.assistant.MainActivity"
            android:label="@string/title_activity_main" />
        <activity android:name="com.example.pm.assistant.EditProfileActivity" />
    </application>

</manifest>